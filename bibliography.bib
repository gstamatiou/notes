	url = {http://arxiv.org/abs/1910.06627},
	doi = {10.2140/gt.2023.27.3303},
	abstract = {We study Anosov representations whose limit set has intermediate regularity, namely is a Lipschitz submanifold of a flag manifold. We introduce an explicit linear functional, the unstable Jacobian, whose orbit growth rate is integral on this class of representations. We prove that many interesting higher rank representations, including \${\textbackslash}Theta\$-positive representations, belong to this class, and establish several applications to rigidity results on the orbit growth rate in the symmetric space.},
	number = {8},
	urldate = {2024-03-28},
